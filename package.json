{
  "name": "the-hampstead-design-journal",
  "version": "1.0.0",
  "description": "A high-end digital publication focused on architecture, interiors, and living in North West London.",
  "main": "src/app/page.tsx",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:update": "jest --updateSnapshot",
    "validate": "npm run lint && npm run type-check && npm run test:ci",
    "prepare": "npm run build"
  },
  "keywords": [
    "architecture",
    "interiors",
    "design",
    "North West London",
    "publication",
    "Hampstead",
    "heritage restoration",
    "planning policy"
  ],
  "author": "The Hampstead Design Journal",
  "license": "MIT",
  "dependencies": {
    "@braintree/sanitize-url": "^7.1.1",
    "@emotion/is-prop-valid": "^1.4.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/leaflet": "^1.9.21",
    "autoprefixer": "^10.4.0",
    "clsx": "^2.1.1",
    "contentlayer": "^0.3.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.26.2",
    "fuse.js": "^7.0.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.555.0",
    "next": "13.5.6",
    "next-contentlayer": "^0.3.4",
    "postcss": "^8.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.0",
    "zod": "^3.23.0"
